{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "https://app.corporacionfamilia.org:4500",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Firefox",
      "request": "launch",
      "type": "firefox",
      "url": "https://app.corporacionfamilia.org:4500",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack:///projects/madsis",
          "path": "${workspaceFolder}/projects/madsis"
        }
      ]
    },
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "msedge",
      "url": "https://app.corporacionfamilia.org:4500",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
